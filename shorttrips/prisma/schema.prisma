generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Booking {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  checkIn   DateTime
  checkOut  DateTime
  roomType  String
  createdAt DateTime @default(now())
}
